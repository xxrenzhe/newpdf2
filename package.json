    "opentype.js": "^1.3.4",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "5.4.296",
    "pdfjs-dist-v2": "npm:pdfjs-dist@2.15.349",
    "qpdf-wasm": "^0.1.0",
    "rangy": "^1.3.2",
