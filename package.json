{
  "name": "nextjs-shadcn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/postinstall.mjs",
    "clean:next": "node scripts/clean-next.mjs",
    "dev": "npm run clean:next && npm run build:pdfeditor && NEXT_IGNORE_INCORRECT_LOCKFILE=1 next dev -H 0.0.0.0",
    "dev:turbopack": "npm run clean:next && npm run build:pdfeditor && NEXT_IGNORE_INCORRECT_LOCKFILE=1 next dev -H 0.0.0.0 --turbopack",
    "build:pdfeditor": "NODE_ENV=production BASE_URL=/pdfeditor/ webpack --config packages/pdfeditor/webpack.build.js",
    "build": "npm run build:pdfeditor && NEXT_IGNORE_INCORRECT_LOCKFILE=1 next build",
    "start": "NEXT_IGNORE_INCORRECT_LOCKFILE=1 next start",
    "prelint": "node scripts/ensure-next-types.mjs",
    "lint": "tsc --noEmit && next lint",
    "test:e2e": "playwright test",
    "format": "biome format --write"
  },
